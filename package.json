    "build:base": "yarn workspace @ix/base dev",
    "prepare:base": "yarn workspace @ix/base prepare",
    "kill": "npx kill-port 3000 3333 8001 8002",
    "killmac": "kill -9 $(lsof -ti:3000,3333,8001,8002)"
  },
