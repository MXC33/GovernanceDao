{
  "name": "ix-apps",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "^18.12.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"yarn:dev:*\" -c \"#C3000F,#84D4BC\"",
    "dev:mp": "yarn workspace @ix/marketplace dev",
    "dev:base": "yarn workspace @ix/base dev",
    "build": "yarn build:mp",
